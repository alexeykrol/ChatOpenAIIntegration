# MaaS Environment Variables Example
# Copy this file to .env and fill in your actual values

# Supabase MaaS Project (отдельный от основного проекта)
MAAS_SUPABASE_URL=https://your-maas-project.supabase.co
MAAS_SUPABASE_ANON_KEY=your-maas-anon-key-here

# n8n Configuration
N8N_WEBHOOK_URL=http://localhost:5678/webhook/maas
N8N_API_KEY=your-n8n-api-key-here

# LLM Configuration for MaaS
OPENAI_API_KEY=sk-proj-your-openai-key-here
CLAUDE_API_KEY=your-claude-key-here

# MaaS Configuration
MAAS_DEFAULT_MODEL=gpt-3.5-turbo
MAAS_TOKEN_BUDGET=4000
MAAS_CACHE_TTL=900